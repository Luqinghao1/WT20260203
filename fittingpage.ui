   <string notr="true">/* 全局设置：白色背景，黑色字体 */
QWidget {
    background-color: #ffffff;
    color: #333333;
    font-family: &quot;Microsoft YaHei&quot;, &quot;Segoe UI&quot;, sans-serif;
    font-size: 9pt;
}

/* 顶部工具栏：纯白背景，底部细灰线 */
QFrame#frameToolbar {
    background-color: #ffffff;
    border: none;
    border-bottom: 1px solid #e0e0e0;
}

/* 按钮样式优化 */
QPushButton {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 4px;
    padding: 5px 15px;
    color: #333333;
}
QPushButton:hover {
    background-color: #f5f5f5;
    border-color: #0078d7;
    color: #0078d7;
}
QPushButton:pressed {
    background-color: #e0e0e0;
}

/* Tab控件整体背景设为白色，消除灰色死角 */
QTabWidget {
    background-color: #ffffff;
}

/* Tab页签容器背景设为白色 */
QTabWidget::pane {
    border: none;
    background: #ffffff;
}

/* Tab条背景设为白色 (解决标题右侧不是白色的问题) */
QTabBar {
    background: #ffffff;
}

/* 具体Tab页签样式 */
QTabBar::tab {
    background: #f9f9f9;
    border: 1px solid #e0e0e0;
    border-bottom: none;
    padding: 6px 15px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    color: #666666;
    min-width: 80px;
}
QTabBar::tab:selected {
    background: #ffffff;
    color: #0078d7;
    border-bottom: 2px solid #0078d7;
    /* 选中时底部高亮 */
    font-weight: bold;
}
QTabBar::tab:hover {
    background: #ffffff;
}
</string>
